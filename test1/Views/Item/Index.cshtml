@using test1.Models
@model IEnumerable<Itemlist>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table border="1">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Price</th>
            <th>Category</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var data in Model)
        {
            <tr>
                <td>@data.Id</td>
                <td>@data.Name</td>
                <td>@data.Price</td>
                <td>@data.Category</td>
            </tr>
        }
    </tbody>

</table>

